{
    "name": "samvdb/payum-paynl",
    "type": "library",
    "description": "The Payum extension for PayNL",
    "keywords": ["payment", "paynl"],
    "homepage": "https://payum.forma-pro.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Van der Borght",
            "email": "samvanderborght@gmail.com"
        },
        {
            "name":     "Payum project",
            "homepage": "https://payum.forma-pro.com/"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/Payum/AuthorizeNetAim/contributors"
        }
    ],
    "require": {
        "payum/core": "^1.5",
        "paynl/sdk": "^1.5"
    },
    "require-dev": {
        "payum/core": "^1.5@dev",
        "php-http/guzzle6-adapter": "^1.0",
        "phpunit/phpunit": "^5.7"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Payum\\Paynl": "" }
    },
    "target-dir": "Payum/Paynl",
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
